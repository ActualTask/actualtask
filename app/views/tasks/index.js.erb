$(document).ready(function() {
  var slider = $("#the_slider").slider({
    range: true,
    min: 200,
    max: 500,
    values: [250, 450],
    slide: function(event, ui) {
      $("#thing_price").val(ui.values[0]);
      $("#thing_price2").val(ui.values[1]);
    }
  });
  $("#thing_price").val(slider.slider("values")[0]);
  $("#thing_price2").val(slider.slider("values")[1]);
});
