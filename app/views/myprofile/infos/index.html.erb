        <%= link_to 'Заполнить личные данные', new_myprofile_infos_path, class: 'btn btn-outline-primary btn-sm' %>
    <% else %>
        <%= current_user.info.name %>
        <%= current_user.info.surname %>
        <div><%= current_user.info.about %></div>
    <% end %>

