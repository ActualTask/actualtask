<%= form_for @performer do |f| %>

    <div class="form-group">
      <%= f.label :photo_id_1 %>
      <%= f.file_field :photo_id_1, class: 'form-control' %>
    </div>

    <div class="form-group">
      <%= f.label :photo_id_2 %>
      <%= f.file_field :photo_id_2, class: 'form-control' %>
    </div>

    <div class="form-group">
      <%= f.submit t'.submit', class: 'btn btn-primary' %>
      <%= link_to t('common.back'), :back, class: 'btn btn-outline-secondary' %>
    </div>

<% end %>

